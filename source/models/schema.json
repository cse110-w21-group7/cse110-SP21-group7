{
    "description": "A user's information for their bullet journal",
    "type": "object",
    "name": "string",
    "current_log": "UNIX timestamp", /*timestamp corresponding to date of daily log that should be populated on daily.html
    when next visited*/
    "properties": {
      "collections": {
        "type": "array",
        "items": [
          {
            "type": "string",
            "images": "image",
            "videos": "video"
          }
        ]
      }
    },
    "$defs": {
      "daily-logs": [
        {
          "type": "object",
          "required": [ "date", "description" ],
          "properties": {
            "date": {
              "type": "string",
              "time": "UNIX timestamp",
              "description": "The date of the event."
            },
            "events": [
              {
                "description": "description for event",
                "logType": "event",
                "time": "UNIX timestamp"
              }
            ],
            "tasks": [
                {
                  "description": "description for task",
                  "logType": "task",
                  "finished": true
                }
            ],
            "notes": [
              {
                "description": "description for note",
                "logType": "note"
              }
          ],
            "mood": {
              "type": "number",
              "multipleOf": 1,
              "minimum": 0,
              "exclusiveMaximum": 100,
              "description": "Daily mood on a range of 0-99."
            }
          }
      }]
    }
  }